{
	"name": "tle-bot",
	"type": "WEB",
	"strategy": {
		"type": "DEPLOYMENT_STRATEGY_TYPE_ROLLING"
	},
	"routes": [{
		"port": 8000,
		"path": "/"
	}],
	"ports": [{
		"port": 8000,
		"protocol": "http"
	}],
	"env": [],
	"regions": ["fra"],
	"scalings": [{
		"scopes": ["region:fra"],
		"min": 1,
		"max": 1,
		"targets": []
	}],
	"instance_types": [{
		"scopes": ["region:fra"],
		"type": "free"
	}],
	"health_checks": [{
		"grace_period": 5,
		"interval": 30,
		"restart_limit": 3,
		"timeout": 5,
		"tcp": {
			"port": 8000
		}
	}],
	"volumes": [],
	"file_mounts": [],
	"skip_cache": false,
	"git": {
		"repository": "github.com/shaonproject/Tle-bot",
		"branch": "main",
		"tag": "",
		"sha": "07e81a50bf18415854ccb542e5c397b046270203",
		"build_command": "npm install",
		"run_command": "node index.js",
		"no_deploy_on_push": false,
		"workdir": "",
		"buildpack": {
			"build_command": "npm install",
			"run_command": "node index.js",
			"privileged": false
		}
	}
}
